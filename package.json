{
	"name": "my-app",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync && npm run smui-theme",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@smui-extra/autocomplete": "^6.0.0-beta.16",
		"@smui/circular-progress": "^6.0.0-beta.16",
		"@smui/drawer": "^6.0.0-beta.16",
		"@smui/list": "^6.0.0-beta.16",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"autoprefixer": "^10.4.4",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"firebase-admin": "^10.1.0",
		"postcss": "^8.4.12",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"smui-theme": "^6.0.0-beta.16",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-dnd-action": "^0.9.18",
		"svelte-preprocess": "^4.10.1",
		"tailwind-scrollbar": "^1.3.1",
		"tailwindcss": "^3.0.24",
		"ts-node": "^10.7.0",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2"
	},
	"type": "module",
	"dependencies": {
		"daisyui": "^2.14.1",
		"firebase": "^9.6.11",
		"fs": "0.0.1-security",
		"svelte-canvas": "^0.7.1",
		"svelte-icons-pack": "^1.4.6",
		"uuid": "^8.3.2",
		"uuidv4": "^6.2.13"
	},
	"browser": {
		"fs": false,
		"path": false,
		"os": false
	  }
}
